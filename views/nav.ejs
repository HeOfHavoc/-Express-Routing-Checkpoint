    <title>Document</title>
    <style>
        body {
    font-family: Arial, sans-serif;
    background-color: #f0f0f0;
    padding: 20px;
}
nav {
    background-color: #333;
    padding: 10px;
    border-radius: 5px;
    text-align: center;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
}
nav a {
    width: 315px;
    color: white;
    padding: 10px 20px;
    text-decoration: none;
    font-size: 16px;
    margin-right: 10px;
    background-color: #4CAF50;
    border-radius: 5px;
}
nav button {
    width: 355px;
    color: white;
    padding: 10px 20px;
    font-size: 16px;
    background-color: #FF5733;
    border: none;
    border-radius: 5px;
    cursor: pointer;
}
